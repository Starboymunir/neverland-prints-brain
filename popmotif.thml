<script>
document.addEventListener('DOMContentLoaded', () => {
  const sliderEl = document.querySelector('.product__media-list--auto-enabled');

  if (sliderEl && sliderEl.swiper) {
    // Disable Swiper auto-rotation
    sliderEl.swiper.params.autoplay = false;

    // Optional: make sure active slide video plays automatically
    const activeSlide = sliderEl.swiper.slides[sliderEl.swiper.activeIndex];
    const video = activeSlide.querySelector('video');
    if (video) {
      video.muted = true;       // required for autoplay
      video.playsInline = true; // mobile fix
      video.play().catch(err => console.warn('Autoplay blocked:', err));
    }

    // Listen to slide changes to play videos
    sliderEl.swiper.on('slideChangeTransitionStart', () => {
      sliderEl.swiper.slides.forEach(slide => {
        const vid = slide.querySelector('video');
        if (vid) {
          vid.pause();
          vid.currentTime = 0;
        }
      });
      const activeVid = sliderEl.swiper.slides[sliderEl.swiper.activeIndex].querySelector('video');
      if (activeVid) activeVid.play().catch(err => console.warn(err));
    });
  }
});
</script>

<script>
function playSlideVideo(slide) {
  const video = slide.querySelector('video');
  // Skip videos with the "no-autoplay-control" class
  if (!video || video.classList.contains('no-autoplay-control')) return;

  video.muted = true;       // required for autoplay
  video.playsInline = true; // mobile fix
  video.play().catch(err => console.warn('Video autoplay blocked:', err));
}

// When pausing videos on slide change
swiper.slides.forEach(slide => {
  const vid = slide.querySelector('video');
  // Skip videos that should not be controlled
  if (!vid || vid.classList.contains('no-autoplay-control')) return;

  vid.pause();
  vid.currentTime = 0;
});
</script>

<script>
  // Select all videos with this class
const autoplayVideos = document.querySelectorAll('video.always-autoplay');

autoplayVideos.forEach((video) => {
  // Try to play the video immediately
  video.play().catch(() => {});

  // Listen for any pause events and immediately play again
  video.addEventListener('pause', () => {
    video.play().catch(() => {});
  });

  // Optional: keep playing if visibility changes
  document.addEventListener('visibilitychange', () => {
    if (!document.hidden) {
      video.play().catch(() => {});
    }
  });
});
</script>


    
