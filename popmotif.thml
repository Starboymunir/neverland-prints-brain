 <script>
  function formatNumber(num) {
    return num.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
  }

  function animateCountUp(el, targetStr, duration = 2000) {
    const hasPlus = targetStr.includes('+');
    const target = parseInt(targetStr.replace(/\D/g, ''), 10);
    const startTime = performance.now();

    function update(currentTime) {
      const elapsed = currentTime - startTime;
      if (elapsed < duration) {
        const progress = elapsed / duration;
        const currentCount = Math.floor(progress * target);
        el.textContent = formatNumber(currentCount);
        requestAnimationFrame(update);
      } else {
        el.textContent = formatNumber(target) + (hasPlus ? '+' : '');
      }
    }

    requestAnimationFrame(update);
  }

  const counters = document.querySelectorAll('.count-up span');

  // Set up Intersection Observer
  const options = {
    root: null, // viewport
    rootMargin: '0px',
    threshold: 0.1 // trigger when 10% visible
  };

  const observer = new IntersectionObserver((entries, observer) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        const el = entry.target;
        animateCountUp(el, el.getAttribute('data-target'));
        observer.unobserve(el); // Stop observing once animated
      }
    });
  }, options);

  counters.forEach(counter => {
    observer.observe(counter);
  });
</script>

